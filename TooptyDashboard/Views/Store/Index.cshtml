@model IEnumerable<TooptyDashboard.Models.Categorie>
@{
    ViewBag.Title = "Store";
}
<h3>Browse Genres</h3>

<p>
    Select from @Model.Count() genres:
</p>
<ul>
    @foreach (var catagorie in Model)
    {
        <li>@Html.ActionLink(catagorie.NomCategorie, "Browse", new { catagorie = catagorie.NomCategorie })</li>
    }
</ul>
